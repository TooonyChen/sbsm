{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9095",
      "external_ui": "/etc/sing-box/ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "直连",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "path": "/etc/sing-box/cache.db",
      "store_fakeip": false
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_direct",
        "type": "udp",
        "server": "119.29.29.29"
      },
      {
        "tag": "dns_proxy",
        "type": "udp",
        "server": "8.8.4.4",
        "detour": "TCP代理"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "dns_direct"
      },
      {
        "clash_mode": "global",
        "server": "dns_proxy"
      },
      {
        "rule_set": "geosite-cn",
        "action": "route",
        "server": "dns_direct"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "dns_proxy"
      }
    ],
    "final": "dns_proxy",
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "tun",
      "address": "172.19.0.1/30",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2085
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2086,
      "sniff": true,
      "users": []
    }
  ],
  "outbounds": [
    {
      "tag": "TCP代理",
      "type": "selector",
      "outbounds": [
        "自动选择"
      ]
    },
    {
      "tag": "UDP代理",
      "type": "selector",
      "outbounds": [
        "TCP代理",
        "自动选择"
      ]
    },
    {
      "tag": "老中IP",
      "type": "selector",
      "outbounds": [
        "直连",
        "TCP代理"
      ]
    },
    {
      "tag": "OpenAI",
      "type": "selector",
      "outbounds": [
        "TCP代理",
        "自动选择"
      ]
    },
    {
      "tag": "YouTube",
      "type": "selector",
      "outbounds": [
        "TCP代理",
        "自动选择"
      ]
    },
    {
      "tag": "漏网之鱼",
      "type": "selector",
      "outbounds": [
        "TCP代理",
        "直连"
      ]
    },
    {
      "tag": "自动选择",
      "type": "urltest",
      "outbounds": []
    },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": [
        "TCP代理"
      ]
    },
    {
      "tag": "直连",
      "type": "direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "漏网之鱼",
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "clash_mode": "direct",
        "outbound": "直连"
      },
      {
        "clash_mode": "global",
        "outbound": "GLOBAL"
      },
      {
        "domain": [
          "clash.razord.top",
          "yacd.metacubex.one",
          "yacd.haishan.me",
          "d.metacubex.one"
        ],
        "outbound": "自动选择"
      },
      {
        "rule_set": "geoip-private",
        "outbound": "直连"
      },
      {
        "rule_set": [
          "geoip-cn",
          "geosite-cn"
        ],
        "outbound": "老中IP"
      },
      {
        "rule_set": "geosite-chat",
        "outbound": "OpenAI"
      },
      {
        "rule_set": "geosite-youtube",
        "outbound": "YouTube"
      },
      {
        "network": [
          "udp"
        ],
        "action": "route",
        "outbound": "UDP代理"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "outbound": "TCP代理"
      }
