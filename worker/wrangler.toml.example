name = "sbsm-worker"
main = "src/index.ts"
compatibility_date = "2025-10-11"

[[d1_databases]]
binding = "DB"
database_name = "sbsm"
database_id = "YOUR_D1_DATABASE_ID"
migrations_dir = "../migrations"

[vars]
ADMIN_USERNAME = "admin"
# SHA-256 hash of your admin password in hex (e.g., echo -n 'password' | shasum -a 256)
ADMIN_PASSWORD_HASH = "REPLACE_WITH_SHA256_HASH"
